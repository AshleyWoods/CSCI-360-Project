<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" 
			prefWidth="600.0" 
			xmlns:fx="http://javafx.com/fxml/1" 
			xmlns="http://javafx.com/javafx/2.2" 
			fx:controller="edu.cofc.Domain.Login.AdminLoginController">
  <children>
    <Button layoutX="200.0" 
    		layoutY="350.0" 
    		mnemonicParsing="false" 
    		onAction="#handleLoginClick" 
    		text="Login" />
   
    <!-- user name -->
    <TextField layoutX="250.0" 
    			layoutY="146.0" 
    			prefWidth="200.0" />
    			
    <!-- password -->
    <TextField layoutX="250.0" 
    		   layoutY="215.0" 
    		   prefWidth="200.0" />
    
    <Text layoutX="175.0" 
    	  layoutY="160.0" 
    	  strokeType="OUTSIDE" 
    	  strokeWidth="0.0" 
    	  text="Username" />
    
    <Text layoutX="175.0" 
    	  layoutY="230.0" 
    	  strokeType="OUTSIDE" 
    	  strokeWidth="0.0" 
    	  text="Password" />
  </children>
</AnchorPane>
