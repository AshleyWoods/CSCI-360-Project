<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="550.0" prefWidth="600.0" 
			xmlns:fx="http://javafx.com/fxml/1" 
			xmlns="http://javafx.com/javafx/2.2" 
			fx:controller="edu.cofc.Login.Controller.LoginController"
			styleClass = "header-section">
  <children>
    <Button mnemonicParsing="false" 
    		onAction="#handleContinueClick" 
    		text="Continue" 
    		AnchorPane.bottomAnchor="15.0" 
    		AnchorPane.rightAnchor="15.0" />
    		
    <Button id="Register" 
    		layoutX="75.0" 
    		layoutY="336.0" 
    		mnemonicParsing="false" 
    		onAction="#handleRegisterClick" 
    		prefHeight="45.0" 
    		prefWidth="290.0" 
    		text="Register To Vote" />
     
    <Button id="registrationInvestigation" 
    		layoutX="300.0" 
    		layoutY="336.0" 
    		mnemonicParsing="false" 
    		onAction="#handleRegistrationInvestigation" 
    		prefHeight="45.0" 
    		prefWidth="290.0" 
    		text="Am I registered?" />
    
    <ComboBox fx:id="loginChoice" 
    		  layoutX="117.0" 
    		  layoutY="210.0" 
    		  prefHeight="45.0" 
    		  prefWidth="290.0" 
    		  promptText="Choose One">
      <items>
        <FXCollections fx:factory="observableArrayList">
          <String fx:value="Driver's Licence" />
          <String fx:value="Social Security Number" />
          <String fx:value="Voter Registration Number" />
          <String fx:value="Administrator Login" />
        </FXCollections>
      </items>
      
    </ComboBox>
   
    
  		<Text text="SC E-Voter"
  			   layoutX ="120"
  			   layoutY = "100"></Text>
   
 
		<Label text="Provide Identification Using:"
			   layoutX = "100"
			   layoutY = "155"
			   >
		</Label>
		
	


  </children>
</AnchorPane>
