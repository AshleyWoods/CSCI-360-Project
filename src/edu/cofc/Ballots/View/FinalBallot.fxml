<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" 
			prefWidth="600.0" 
			xmlns:fx="http://javafx.com/fxml/1" 
			xmlns="http://javafx.com/javafx/2.2" 
			fx:controller="edu.cofc.Ballots.Controller.FinalBallotController"
			styleClass = "ballot-section">
  <children>
    <Button layoutX="350.0" 
    	    layoutY="500.0" 
    	    mnemonicParsing="false" 
    	    onAction="#handleSubmitClick" 
    	    text="Submit" />
    	    
    <Button layoutX="30.0" 
    		layoutY="500.0" 
    		mnemonicParsing="false" 
    		onAction="#handleCancelClick" 
    		text="Cancel" />
    		
    <Button layoutX="10.0" 
    		layoutY="7.0" 
    		mnemonicParsing="false" 
    		onAction="#handleBackClick" 
    		text="Back" />
    <GridPane layoutX="14.0" 
    		  layoutY="62.0" 
    		  prefHeight="259.0" 
    		  prefWidth="572.0">
      <children>
        <RadioButton fx:id="peterParker" 
        			 mnemonicParsing="false" 
        			 selected="false" text="Peter Parker" 
        			 GridPane.columnIndex="0" GridPane.rowIndex="1">
          <toggleGroup>
            <ToggleGroup fx:id="buttonGroup" />
          </toggleGroup>
        </RadioButton>
        <RadioButton fx:id="batMan" 
        			 mnemonicParsing="false" 
        			 text="Batman" 
        			 toggleGroup="$buttonGroup" 
        			 GridPane.columnIndex="0" GridPane.rowIndex="2" />
        			 
        <RadioButton fx:id="voldemort" 
        			 mnemonicParsing="false" 
        			 text="Spider Man" 
        			 toggleGroup="$buttonGroup" 
        			 GridPane.columnIndex="2" GridPane.rowIndex="1" />
        			 
        <RadioButton fx:id="bruceWayne" 
        			 mnemonicParsing="false" 
        			 text="Bruce Wayne" 
        			 toggleGroup="$buttonGroup" 
        			 GridPane.columnIndex="2" GridPane.rowIndex="2" />
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
    </GridPane>
    <Text layoutX="70.0" 
    	  layoutY="100.0" 
    	  strokeType="OUTSIDE" 
    	  strokeWidth="0.0" 
    	  text="Senate Representative Election" />
  </children>
</AnchorPane>
